{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        ".next/**",
        "storybook-static/**"
      ]
    },
    "dev": {
      "cache": false
    },
    "storybook": {
      "cache": false
    },
    "lint": {
      "outputs": []
    }
  }
}
